{
  "configurations": [
    {
      "name": "macos-clang-x64",
      "includePath": [
        "${workspaceFolder}/include",
        "${workspaceFolder}/src",
        "${workspaceFolder}/**"
      ],
      "browse": {
        "path": [
          "${workspaceFolder}/include",
          "${workspaceFolder}/src"
        ],
        "limitSymbolsToIncludedHeaders": true
      },
      "compilerPath": "/usr/bin/clang",
      "cStandard": "${default}",
      "cppStandard": "c++17",
      "intelliSenseMode": "macos-clang-x64",
      "compilerArgs": [
        "-I${workspaceFolder}/include"
      ]
    }
  ],
  "version": 4
}
